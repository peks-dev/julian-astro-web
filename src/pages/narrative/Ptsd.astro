---
import Layout from "../../layouts/Layout.astro";
import ProjectLayout from "../../layouts/project-details/ProjectLayout.astro";
import { projectsData } from "../../context/projectsData";
---

<Layout
  title={projectsData[6].name}
  metaTitle={projectsData[6].name}
  metaDescription={projectsData[6].description}
  metaImage={projectsData[6].metaImage}
>
  <ProjectLayout
    projectTitle={projectsData[6].name}
    projectCategory={projectsData[6].category}
    projectDetails={projectsData[6].teamMates}
    images={projectsData[6].images}
  />
</Layout>
