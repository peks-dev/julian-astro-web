---
import Layout from "../../layouts/Layout.astro";
import ProjectLayout from "../../layouts/project-details/ProjectLayout.astro";
import { projectsData } from "../../context/projectsData";
---

<Layout
  title={projectsData[5].name}
  metaTitle={projectsData[5].name}
  metaDescription={projectsData[5].description}
  metaImage={projectsData[5].metaImage}
>
  <ProjectLayout
    projectTitle={projectsData[5].name}
    projectCategory={projectsData[5].category}
    projectDetails={projectsData[5].teamMates}
    images={projectsData[5].images}
  />
</Layout>
