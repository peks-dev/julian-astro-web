---
import Layout from "../../layouts/Layout.astro";
import ProjectLayout from "../../layouts/project-details/ProjectLayout.astro";
import { projectsData } from "../../context/projectsData";
---

<Layout
  title={projectsData[1].name}
  metaTitle={projectsData[1].name}
  metaDescription={projectsData[1].description}
  metaImage={projectsData[1].metaImage}
>
  <ProjectLayout
    projectTitle={projectsData[1].name}
    projectCategory={projectsData[1].category}
    projectDetails={projectsData[1].teamMates}
    images={projectsData[1].images}
  />
</Layout>
